    "mini-css-extract-plugin": "^2.4.5",
    "node-sass": "^7.0.0",
    "openapi-typescript": "^4.4.0",
    "prettier": "^2.5.1",
    "type-fest": "^2.8.0",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
