    "grommet-icons": "^4.7.0",
    "html-webpack-plugin": "^5.5.0",
    "i18next": "^21.8.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.1",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "styled-components": "^5.3.5",
    "ts-loader": "^9.2.6"
  },
  "devDependencies": {
