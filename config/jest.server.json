{
    "moduleFileExtensions": [
        "js",
        "json",
        "ts",
        "tsx"
    ],
    "rootDir": "../src/server",
    "testRegex": ".spec.tsx?$",
    "transform": {
        "^.+\\.tsx?$": ["ts-jest", {"tsconfig": "./tsconfig.server.json"}]
    },
    "coverageDirectory": "<rootDir>/../coverage",
    "testEnvironment": "node"
}
